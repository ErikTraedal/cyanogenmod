<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- You can have as many remote repositories as you like -->
  <remote name="jianminglok" fetch="git://github.com/jianminglok/" />

  <!-- List all the repositories you want to fetch -->
  <!-- "name": git repository's name on hosting server
       "remote": the remote host from above (if not the default)
       "revision": commitish to checkout (if not the default)
       "path": directory where git repository gets cloned/synced -->
  <project name="android_device_xiaomi_hermes" revision="cm-12.1" remote="jianminglok" path="device/xiaomi/hermes" />
  <project name="android_vendor_xiaomi_hermes" revision="cm-12.1" remote="jianminglok" path="vendor/xiaomi/hermes" /> 
  <project name="android_vendor_mediatek" revision="cm-12.1" remote="jianminglok" path="vendor/mediatek" />
</manifest>

